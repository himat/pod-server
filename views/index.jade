extends layout

script
  src="https://cdn.socket.io/socket.io-1.3.5.js"

script.
  var socket = io();
  //var socket = io.connect("http://localhost");
  socket.on('news', function(data) {
    console.log(data);
    socket.emit('otherevent', { my : 'data'});
  });
	

block content
  h1= title
  p Welcome to #{title}
  form(method='post', action='/genjsonfile')
    input#btn_gen_json(type='submit', value="Convert CSV to JSON file")
    
  form(method='get', action='/genjson')
    input#btn_gen_json(type='submit', value="Convert CSV and return as JSON")